{
  "name": "zent-docs",
  "version": "0.0.1",
  "description": "doc site generator for zent",
  "private": true,
  "keywords": [
    "docs",
    "react",
    "zent"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "node scripts/gather.js && yarn workspace zent invalidate-ts-build-cache",
    "prepare-build": "rm -rf dist && yarn prepare",
    "dev": "yarn prepare && cross-env NODE_ENV=development node scripts/dev.js",
    "webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.prd.config.js",
    "build": "yarn prepare-build && yarn webpack && yarn workspace zent typedoc",
    "deploy": "yarn build && ./scripts/deploy.sh",
    "lint-js": "eslint webpack scripts",
    "lint-ts": "yarn workspace zent build && yarn prepare && eslint src --ext=.ts,.tsx",
    "webpack-beta": "cross-env NODE_ENV=production VERSION=beta webpack --config webpack/webpack.prd.config.js",
    "build-beta": "yarn prepare-build && yarn webpack-beta && yarn workspace zent typedoc",
    "deploy-beta": "yarn build-beta && ./scripts/deploy-beta.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "classnames": "^2.2.5",
    "core-js": "^3.15.2",
    "fuse.js": "^6.4.6",
    "memoize-one": "^5.2.1",
    "prismjs": "^1.24.0",
    "react": "17.0.x",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "17.0.x",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/parser": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/types": "^7.1.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-rc.0",
    "@types/react": "^17.0.13",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-member-expression-literals": "^6.9.4",
    "babel-plugin-transform-property-literals": "^6.9.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "colors": "^1.1.2",
    "coveralls": "^3.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "eslint-import-resolver-webpack": "^0.13.1",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "front-matter": "^4.0.2",
    "gh-pages": "3.2.3",
    "glob": "^7.1.7",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "markdown-doc-loader": "^2.1.4",
    "mini-css-extract-plugin": "^2.0.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "react-markdown-doc-loader": "^3.0.4",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.1",
    "sass": "^1.22.7",
    "sass-lint": "^1.12.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.3",
    "typescript": "~4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^5.8.0"
  }
}
