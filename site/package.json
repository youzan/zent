{
  "name": "zent-docs",
  "version": "0.0.1",
  "description": "doc site generator for zent",
  "keywords": [
    "docs",
    "react",
    "zent"
  ],
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "dev": "node scripts/gather.js && cross-env NODE_ENV=development node scripts/dev.js",
    "build": "rm -rf dist && node scripts/gather.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack/webpack.prd.config.js",
    "deploy": "yarn run build && ./scripts/deploy.sh"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "fuse.js": "^3.2.0",
    "prismjs": "^1.8.4",
    "prop-types": "^15.6.0",
    "react": "15.5.x",
    "react-dom": "15.5.x",
    "react-loadable": "^5.3.0",
    "react-router-dom": "^4.1.1",
    "transliteration": "^1.6.2"
  },
  "devDependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "colors": "^1.1.2",
    "cross-env": "^3.2.4",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "1",
    "front-matter": "^2.2.0",
    "gh-pages": "^0.12.0",
    "happypack": "4.0.0",
    "html-loader": "^0.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "markdown-doc-loader": "2.1.2",
    "postcss-loader": "2",
    "ramda": "^0.25.0",
    "react-hot-component-loader": "^1.0.3",
    "react-hot-loader": "3",
    "react-markdown-doc-loader": "2.1.0",
    "style-loader": "^0.19",
    "url-loader": "^0.6",
    "webpack": "3",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9"
  }
}
