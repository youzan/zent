{
  "name": "zent-docs",
  "version": "0.0.1",
  "description": "doc site generator for zent",
  "keywords": [
    "docs",
    "react",
    "zent"
  ],
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "dev": "node scripts/gather.js && cross-env NODE_ENV=development node scripts/dev.js",
    "prepare-build": "rm -rf dist && node scripts/gather.js",
    "webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack/webpack.prd.config.js",
    "build": "yarn prepare-build && yarn webpack",
    "deploy": "yarn build && ./scripts/deploy.sh",
    "webpack-beta": "cross-env NODE_ENV=production VERSION=beta webpack --progress --hide-modules --config webpack/webpack.prd.config.js",
    "build-beta": "yarn prepare-build && yarn webpack-beta",
    "deploy-beta": "yarn build-beta && ./scripts/deploy-beta.sh"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "fuse.js": "^3.2.0",
    "prismjs": "1.8.4",
    "prop-types": "^15.6.0",
    "react": "16.7.x",
    "react-dom": "16.7.x",
    "react-loadable": "^5.3.0",
    "react-router-dom": "^4.1.1",
    "transliteration": "^1.6.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "colors": "^1.1.2",
    "cross-env": "^4.0.0",
    "css-loader": "^1.0.1",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^2.0.0",
    "front-matter": "^2.2.0",
    "gh-pages": "1.2.0",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "markdown-doc-loader": "2.1.3",
    "mini-css-extract-plugin": "^0.4.5",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "ramda": "^0.25.0",
    "react-markdown-doc-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}
