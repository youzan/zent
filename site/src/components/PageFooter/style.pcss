@component-namespace page {
  @b footer {
    height: 72px;
    margin-top: 40px;
    background-color: #34383B;
    display: flex;
    justify-content: center;

    @e item {
      float: left;
      margin: 0 20px;
    }

    @e link {
      display: block;
      color: #E5E5E5;
      font-size: 12px;
      line-height: 72px;
    }

    @e copyright {
      color: #999;
      font-size: 12px;
      line-height: 72px;
      margin-left: 50px;
    }
  }
}
