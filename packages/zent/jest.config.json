{
  "verbose": true,
  "automock": false,
  "collectCoverage": true,
  "coverageReporters": [
    "text",
    "text-summary",
    "lcov",
    "html"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 96,
      "branches": 85,
      "functions": 96,
      "lines": 96
    }
  },
  "timers": "fake",
  "testPathIgnorePatterns": [
    "<rootDir>/__tests__/table/comp"
  ],
  "coveragePathIgnorePatterns": [
    "<rootDir>/src/slider/common.js",
    "<rootDir>/src/slider/container.js",
    "<rootDir>/src/slider/points.js",
    "<rootDir>/src/colorpicker/helpers/reactcss",
    "<rootDir>/src/colorpicker/SketchFields.js",
    "<rootDir>/src/colorpicker/SketchPresetColors.js",
    "<rootDir>/src/colorpicker/common/ColorWrap.js",
    "<rootDir>/src/colorpicker/helpers/checkboard.js",
    "<rootDir>/src/utils/scroll.js",
    "<rootDir>/src/utils/getWidth.js"
  ],
  "moduleNameMapper": {
    "^affix(.*)$": "<rootDir>/src/affix$1",
    "^alert(.*)$": "<rootDir>/src/alert$1",
    "^badge(.*)$": "<rootDir>/src/badge$1",
