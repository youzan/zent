@import '../theme/default.pcss';

@mixin popup {
  box-shadow: 0 1px 4px $theme-mask-black-2;
  left: 0;
  outline: none;
  overflow-y: auto;
  position: relative;
  width: 100%;
  z-index: 2000;
  margin-top: 2px;
  background-color: $theme-stroke-9;
  max-height: 224px;
  border-radius: 3px;
  font-size: 12px;
}
