@import './theme/default.pcss';

.zent-sweetalert-icon-title {
  line-height: 14px;

  .zent-sweetalert-type-icon {
    margin-right: 10px;
    vertical-align: bottom;
    font-weight: bold;
  }

  .zenticon-info-circle-o {
    color: $theme-primary-2;
  }

  .zenticon-check-circle-o {
    color: $theme-success-2;
  }

  .zenticon-close-circle-o {
    color: $theme-error-2;
  }

  .zenticon-error-circle-o {
    color: $theme-warn-1;
  }
}
