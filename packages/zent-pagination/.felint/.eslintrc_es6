{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-var": 2,
    "semi": [1, "always", { "omitLastInOneLineBlock": true}],
    "max-len": 0,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "prefer-const": 0,
    "arrow-body-style": 0,
    "react/sort-comp": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "react/prefer-es6-class": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-no-bind": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-unused-expressions": 0
  }
}